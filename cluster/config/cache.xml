<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE cache PUBLIC  "-//GemStone Systems, Inc.//GemFire Declarative Caching 8.0//EN" "http://www.gemstone.com/dtd/cache8_0.dtd">
<cache>
	<gateway-sender id="ToRemote" 
		remote-distributed-system-id="${REMOTE_DISTRIBUTED_SYSTEM_ID}" 
		parallel="false" 
		order-policy="key" />
		
	<gateway-receiver start-port="${GATEWAY_RECEIVER_START_PORT}" end-port="${GATEWAY_RECEIVER_END_PORT}"/>
	
	<region name="Test">
		<region-attributes refid="PARTITION_REDUNDANT" gateway-sender-ids="ToRemote"/>
	</region>
	<region name="RTest">
		<region-attributes refid="REPLICATE" gateway-sender-ids="ToRemote"/>
	</region>
	<region name="TestP">
		<region-attributes refid="PARTITION_REDUNDANT_PERSISTENT" gateway-sender-ids="ToRemote"/>
	</region>
	<region name="RTestP">
		<region-attributes refid="REPLICATE_PERSISTENT" gateway-sender-ids="ToRemote"/>
	</region>
</cache>